

<p id="notice"><%= notice %></p>

 <div class="fluid-container">
      <div class="row">
        <div class="col-sm-2">
          <% if user_signed_in? %>
            <h5>Chatrooms</h5>
            <ul>
              <% current_user.chatrooms.each do |chatroom| %>
                <li><%= link_to chatroom.name, "#" %></li>
              <% end %>
            </ul>
          <% end %>
        </div>

        <div class="col-sm-10">
          <%= render 'chatroom_each'%>
        </div>
      </div>
    </div>



