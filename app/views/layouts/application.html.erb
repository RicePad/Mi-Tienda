<!DOCTYPE html>
<html>
  <head>
    <title>MitiendaApp</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%#= render 'shared/navbar' %>
        <nav class="navbar navbar-default navbar-fixed-top react-nav">
          <%= javascript_pack_tag 'navbar_react'%>
        </nav>

        

      <div id="react-component">
      	<%= javascript_pack_tag 'application'%>
      </div>
    <%= yield %>
      <%#= render 'shared/footer' %>
      <div id="react-footer">
        <%= javascript_pack_tag 'footer_react'%>
      </div>



  </body>
</html>
