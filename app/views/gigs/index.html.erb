
 <div id="gig-list">
           <%= javascript_pack_tag 'gig_react'%>
  </div>


<% @gig_items.each do |gig| %>
	<div class="col-md-3">
		<div class="thumbnail">
			<%= image_tag gig.thumb_image %>
		</div>
		<div class="caption">
			<p>
			 <%= gig.description %>
			 <span>by RicePad</span>
			 <b class="green pull-right"><%= gig.price %></b>

			</p>
		</div>
	</div>
<% end %>